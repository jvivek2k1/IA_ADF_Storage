{
	"name": "Copy Data",
	"properties": {
		"activities": [
			{
				"name": "Copy Data1",
				"description": "First Time ADF",
				"type": "Copy",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 3,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "SqlSource"
					},
					"sink": {
						"type": "SqlSink",
						"writeBatchSize": 10000
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"columnMappings": {
							"FirstName": "FirstName",
							"MiddleName": "MiddleName",
							"LastName": "LastName"
						}
					}
				},
				"inputs": [
					{
						"referenceName": "JB_SOURCE",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "JB_Azure",
						"type": "DatasetReference"
					}
				]
			}
		]
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}