{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "jb-adf-eastus-dev"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/Ilog_Lookup')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "TableLoadDetails_Lookup",
						"description": "Check [TableLoadDetails] to look for tables that should be loaded.",
						"type": "Lookup",
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 3,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [
							{
								"name": ""
							}
						],
						"typeProperties": {
							"source": {
								"type": "SqlSource",
								"sqlReaderQuery": "SELECT [iLogID]\n      ,[sSourceSchemaName]+'.'+[sSourceTableName] as SourceTable\n      ,[sDestinationSchemaName]+'.'+[sDestinationTableName] as DestinationTable\n  FROM [ELT].[TableLoadDetails]"
							},
							"dataset": {
								"referenceName": "Azure_TableLoadDetails",
								"type": "DatasetReference"
							},
							"firstRowOnly": false
						}
					}
				],
				"folder": {
					"name": "IA"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/Azure_TableLoadDetails')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/Azure_TableLoadDetails')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzureSQLDB",
					"type": "LinkedServiceReference"
				},
				"type": "AzureSqlTable",
				"typeProperties": {
					"tableName": "[concat('[', 'ELT].[TableLoadDetails]')]"
				}
			}
		}
	]
}